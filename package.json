{
  "name": "svelte-pdf-simple",
  "author": {
    "name": "Georgi Spasov"
  },
  "version": "1.0.7",
  "description": "Svelte component for displaying PDF files easily",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "dependencies": {
    "pdfjs-dist": "2.13.216"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gspasov/svelte-pdf-simple"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gspasov/svelte-pdf-simple/issues"
  },
  "homepage": "https://github.com/gspasov/svelte-pdf-simple#readme",
  "keywords": [
    "pdf",
    "svelte-pdf",
    "pdfjs",
    "svelte-pdf",
    "pdf-simple",
    "pdf-viewer",
    "svelte-pdf-viewer",
    "svelte-simple-pdf",
    "svelte-pdf-simple",
    "pdf-print",
    "pdf-zoom",
    "pdf-rotation",
    "pdf-navigation"
  ]
}
